<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    android:clickable="true"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/rang_contants_dialog_selected_shape"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_dialog_title"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:gravity="center"
                android:text=""
                android:textColor="#8c8c8c"
                android:textSize="12sp"
                android:visibility="gone"
                tools:visibility="visible"/>

            <View
                android:id="@+id/divider_title"
                android:layout_width="match_parent"
                android:layout_height="0.2dp"
                android:background="@color/miaobian_xian"
                android:visibility="gone"
                tools:visibility="visible"/>

            <TextView
                android:id="@+id/tv_item_1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_43"
                android:background="@drawable/diy_text_bg_selector"
                android:gravity="center"
                android:textColor="@color/text_nick_name"
                android:textSize="15sp"
                android:visibility="gone"
                tools:visibility="visible"/>

            <View
                android:id="@+id/divider_1"
                android:layout_width="match_parent"
                android:layout_height="0.2dp"
                android:background="@color/miaobian_xian"
                android:visibility="gone"
                tools:visibility="visible"/>

            <TextView
                android:id="@+id/tv_item_2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_43"
                android:background="@drawable/diy_text_bg_selector"
                android:gravity="center"
                android:text=" "
                android:textColor="@color/text_nick_name"
                android:textSize="15sp"
                android:visibility="gone"
                tools:visibility="visible"/>

            <View
                android:id="@+id/divider_2"
                android:layout_width="match_parent"
                android:layout_height="0.2dp"
                android:background="@color/miaobian_xian"
                android:visibility="gone"
                tools:visibility="visible"/>

            <TextView
                android:id="@+id/tv_item_3"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_43"
                android:background="@drawable/diy_text_bg_selector"
                android:gravity="center"
                android:text=" "
                android:textColor="@color/text_nick_name"
                android:textSize="15sp"
                android:visibility="gone"
                tools:visibility="visible"/>

            <View
                android:id="@+id/divider_3"
                android:layout_width="match_parent"
                android:layout_height="0.2dp"
                android:background="@color/miaobian_xian"
                android:visibility="gone"
                tools:visibility="visible"/>

            <TextView
                android:id="@+id/tv_item_4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_43"
                android:background="@drawable/diy_text_bg_selector"
                android:gravity="center"
                android:text=" "
                android:textColor="@color/text_nick_name"
                android:textSize="15sp"
                android:visibility="gone"
                tools:visibility="visible"/>

            <View
                android:id="@+id/divider_4"
                android:layout_width="match_parent"
                android:layout_height="0.2dp"
                android:background="@color/miaobian_xian"
                android:visibility="gone"
                tools:visibility="visible"/>

            <TextView
                android:id="@+id/tv_item_5"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_43"
                android:background="@drawable/diy_text_bg_selector"
                android:gravity="center"
                android:text=" "
                android:textColor="@color/text_nick_name"
                android:textSize="15sp"
                android:visibility="gone"
                tools:visibility="visible"/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="#f4f4f5" />

        <TextView
            android:id="@+id/tv_dialog_cancel"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_43"
            android:background="@drawable/diy_text_bg_selector"
            android:gravity="center"
            android:text="@string/cancel"
            android:textColor="@color/text_nick_name"
            android:textSize="15sp" />

    </LinearLayout>

</FrameLayout>