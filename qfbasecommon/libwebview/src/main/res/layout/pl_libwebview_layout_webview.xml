<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.rongtuoyouxuan.chatlive.webview.NativeWebView
        android:id="@+id/web_view_wrapper_nativeWebView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />

    <com.rongtuoyouxuan.libuikit.CommonLoadErrorLayout
        android:id="@+id/web_view_wrapper_layout_load_error"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.rongtuoyouxuan.libuikit.CommonLoadingLayout
        android:id="@+id/web_view_wrapper_layout_loading"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible" />
</FrameLayout>